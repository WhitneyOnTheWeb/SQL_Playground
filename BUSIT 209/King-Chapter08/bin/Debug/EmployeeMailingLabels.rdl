  <DataSets>
    <DataSet Name="Employees">
      <Query>
        <DataSourceName>Galactic</DataSourceName>
        <CommandText>SELECT FirstName + ' ' + LastName AS Name,
	Address1,
	City + ', ' + State + ' ' + ZipCode AS CSZ
FROM Employee
WHERE TerminationDate IS NULL
ORDER BY ZipCode, LastName, FirstName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
