  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Galactic</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BillingCity">
            <Value>=Parameters!BillingCity.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Customer.BillingCity, Customer.Name, InvoiceHeader.BatchNumber, InvoiceHeader.InvoiceNumber, InvoiceHeader.TotalAmount, InvoiceBatch.RunDate
FROM            Customer INNER JOIN
                         InvoiceHeader ON Customer.CustomerNumber = InvoiceHeader.CustomerNumber INNER JOIN
                         InvoiceBatch ON InvoiceHeader.BatchNumber = InvoiceBatch.BatchNumber
WHERE        (Customer.BillingCity = @BillingCity) AND (InvoiceBatch.RunDate &gt;= @StartDate AND InvoiceBatch.RunDate &lt; DATEADD(dd, 1, @EndDate))</CommandText>
      </Query>
      <Fields>
        <Field Name="BillingCity">
          <DataField>BillingCity</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BatchNumber">
          <DataField>BatchNumber</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="InvoiceNumber">
          <DataField>InvoiceNumber</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TotalAmount">
          <DataField>TotalAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="RunDate">
          <DataField>RunDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="BillingCities">
      <Query>
        <DataSourceName>Galactic</DataSourceName>
        <CommandText>SELECT DISTINCT BillingCity FROM Customer</CommandText>
      </Query>
      <Fields>
        <Field Name="BillingCity">
          <DataField>BillingCity</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
