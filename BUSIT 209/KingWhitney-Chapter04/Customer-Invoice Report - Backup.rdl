        </Field>
      </Fields>
      <Query>
        <DataSourceName>Galactic</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        Customer.BillingCity, Customer.Name, InvoiceHeader.InvoiceNumber, InvoiceHeader.InvoiceDate, InvoiceHeader.TotalAmount
FROM            Customer LEFT OUTER JOIN
                         InvoiceHeader ON Customer.CustomerNumber = InvoiceHeader.CustomerNumber
ORDER BY Customer.BillingCity, Customer.Name, InvoiceHeader.InvoiceNumber</CommandText>
        <Timeout>0</Timeout>
      </Query>
