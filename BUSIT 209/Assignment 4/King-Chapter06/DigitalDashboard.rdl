        <CommandText>SELECT COUNT(Delivery.DeliveryNumber) AS NumOfPickups,
	COUNT(LostPackage.LostPackageNumber) AS NumLost,
	(SELECT COUNT(*)
	FROM Repair
	WHERE BeginWorkDate
	BETWEEN DATEADD(d, -28, @GaugeDate) AND @GaugeDate)
	AS NumRepairs
FROM Delivery
LEFT OUTER JOIN LostPackage
	ON Delivery.DeliveryNumber = LostPackage.DeliveryNumber
WHERE PickupDateTime BETWEEN DATEADD(d, -28, @GaugeDate)
AND @GaugeDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
